{ "name": "media from text file",
  "interface": {
      "intro": {
	  "title": "media from text file submission",
	  "text": "media upload requires a text file (with '.txt' extension); format 'Mg 0.05 -100 100' or 'cpd12345 0.05 -100 100'",
	  "excel": false,
	  "json": false
      },
      "inputs": [
	  { 
	      "type": "text",
	      "label": "Enter media id",
	      "help": null,
	      "default": "",
	      "data": null,
	      "isMetadata": false,
	      "aweVariable": "MEDIA"
	  },
	  { 
	      "type": "dropdown",
	      "filetype": [ "txt" ],
	      "label": "select media text file",
	      "help": null,
	      "default": null,
	      "data": null,
	      "isMetadata": false,
	      "aweVariable": "MEDIAFILE"
	  }
      ]
  },
  "awe": {
      "tasks": [
	  {
	    "cmd": {
		  "args": "@##MEDIA## @##MEDIAFILEFileName## -w ##WORKSPACE##",
		  "description": "save data as media typed object",
		  "name": "fba-addmedia.pl",
	  "environ": {"KB_AUTH_TOKEN":""}
	      },
	      "dependsOn": [ ],
	      "inputs": {
		  "##MEDIAFILEFileName##": {
		      "host": "##SHOCK##",
		      "node": "##MEDIAFILE##"
		  },
		  "##MEDIA##": {
		      "host": "##SHOCK##",
		      "node": "##MEDIA##"
		  },
		  "metadatafile": {
		      "host": "##SHOCK##",
		      "node": "##METADATA##"
		  }
	      },
	      "taskid": "0",
	      "totalwork": 1
	  }
      ]
  }
}
